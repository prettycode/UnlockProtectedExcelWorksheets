{
    "name": "protected-worksheet-unlocker",
    "version": "0.1.0",
    "author": {
        "name": "Chris O'Brien"
    },
    "license": "CC BY-NC 4.0",
    "scripts": {
        "clean": "rmdir node_modules /s /q",
        "versions": "echo node: && node -v && echo pnpm: && pnpm -v",
        "preinstall": "npx only-allow pnpm",
        "install:ci": "pnpm install --frozen-lockfile",
        "outdated:ci": "pnpm outdated --long",
        "outdated:fix": "pnpm update --latest",
        "audit:ci": "pnpm audit --audit-level=low",
        "audit:fix": "pnpm audit --fix",
        "prettier:ci": "prettier . --check",
        "prettier:fix": "prettier . --write",
        "lint:ci": "eslint . --max-warnings=0",
        "lint:fix": "eslint . --fix",
        "copy-paste": "jscpd",
        "copy-paste:ci": "jscpd --exitCode 1",
        "build:ci": "pnpm versions && pnpm install:ci && pnpm outdated:ci && pnpm audit:ci && pnpm prettier:ci && pnpm lint:ci && pnpm copy-paste:ci && pnpm test:ci",
        "build:fix": "pnpm versions && pnpm outdated:fix && pnpm audit:fix && pnpm prettier:fix && pnpm lint:fix && pnpm copy-paste && pnpm test",
        "test": "jest --passWithNoTests",
        "test:ci": "jest --ci",
        "start": "ts-node ./src/program.ts",
        "start:fix": "pnpm build:fix && pnpm start",
        "precommit": "pnpm build:fix && pnpm run ci",
        "ci": "pnpm build:ci && pnpm start"
    },
    "dependencies": {
        "adm-zip": "^0.5.14",
        "jsdom": "^24.1.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.7.0",
        "@types/adm-zip": "^0.5.5",
        "@types/jest": "^29.5.12",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^20.14.11",
        "eslint": "^9.7.0",
        "globals": "^15.8.0",
        "jest": "^29.7.0",
        "jscpd": "^4.0.5",
        "prettier": "^3.3.3",
        "ts-jest": "^29.2.3",
        "ts-node": "^10.9.2",
        "typescript-eslint": "^7.16.1"
    }
}
