{
    "name": "unlock-protected-worksheets",
    "version": "0.1.0",
    "author": {
        "name": "Chris O'Brien"
    },
    "license": "CC BY-NC 4.0",
    "type": "module",
    "scripts": {
        "clean": "npx rimraf ./node_modules ./coverage",
        "versions": "echo node: && node -v && echo pnpm: && pnpm -v",
        "install:ci": "pnpm install --frozen-lockfile",
        "outdated:ci": "pnpm outdated --long",
        "outdated:fix": "pnpm update --latest",
        "audit:ci": "pnpm audit --audit-level=low",
        "audit:fix": "pnpm audit --fix",
        "prettier:ci": "prettier . --check",
        "prettier:fix": "prettier . --write",
        "lint:ci": "eslint . --max-warnings=0",
        "lint:fix": "eslint . --fix",
        "copy-paste": "jscpd",
        "copy-paste:ci": "jscpd --exitCode 1",
        "spell-check": "npx cspell . --no-exit-code",
        "spell-check:ci": "npx cspell .",
        "build:ci": "pnpm versions && pnpm install:ci && pnpm outdated:ci && pnpm audit:ci && pnpm prettier:ci && pnpm lint:ci && pnpm copy-paste:ci  && pnpm spell-check:ci && pnpm test:ci",
        "build:fix": "pnpm versions && pnpm outdated:fix && pnpm audit:fix && pnpm prettier:fix && pnpm lint:fix && pnpm copy-paste && pnpm spell-check && pnpm test",
        "test": "jest --passWithNoTests",
        "test:ci": "jest --ci",
        "start": "npx tsx ./src/program.ts --files ../data/Unprotected.xlsx ../data/Protected.xlsx",
        "start:fix": "pnpm build:fix && pnpm start",
        "precommit": "pnpm build:fix && pnpm run ci",
        "ci": "pnpm build:ci && pnpm start"
    },
    "engines": {
        "node": ">=20.15.1",
        "pnpm": ">=9.5.0"
    },
    "dependencies": {
        "adm-zip": "^0.5.14",
        "jsdom": "^24.1.0",
        "ts-command-line-args": "^2.5.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.7.0",
        "@types/adm-zip": "^0.5.5",
        "@types/jest": "^29.5.12",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^20.14.11",
        "cspell": "^8.11.0",
        "eslint": "^9.7.0",
        "globals": "^15.8.0",
        "jest": "^29.7.0",
        "jscpd": "^4.0.5",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.3",
        "ts-node": "^10.9.2",
        "tsx": "^4.16.2",
        "typescript-eslint": "^7.16.1"
    }
}
